<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="https://jakarta.ee/xml/ns/persistence/orm"
                 version="3.0">

    <entity class="com.example.securedigitalticketingsystemmain.core.entity.TicketEntity">
        <table name="ticket" schema="public" catalog="postgres"/>
        <attributes>
            <id name="id">
                <column name="id" precision="-1"/>
            </id>
            <basic name="eventId">
                <column name="event_id" nullable="false" precision="-1"/>
            </basic>
            <basic name="customerId">
                <column name="customer_id" nullable="false" precision="-1"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
